  </servlet>  
  <servlet-mapping>  
    <servlet-name>Access</servlet-name>  
    <url-pattern>/Access</url-pattern>  
  </servlet-mapping>  
  
        <!-- Access Acct Section -->
  <servlet>  
    <servlet-name>AccessAccount</servlet-name>  
    <servlet-class>com.rp.system.login.AccessAccount</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>AccessAccount</servlet-name>  
    <url-pattern>/AccessAccount</url-pattern>  
  </servlet-mapping>  
  
   <!-- Download -->
   <servlet>
   <servlet-name>Download</servlet-name>  
    <servlet-class>com.rp.system.content.Download</servlet-class>  
   </servlet>  
  <servlet-mapping>  
    <servlet-name>Download</servlet-name>  
    <url-pattern>/Download</url-pattern>  
