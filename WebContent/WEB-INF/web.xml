<?xml version="1.0" encoding="UTF-8"?>  
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   
xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">  

<welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
</welcome-file-list>
  
    <!-- Login Section -->
  <servlet>
    <servlet-name>Login</servlet-name>  
    <servlet-class>com.rp.system.login.Login</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>Login</servlet-name>  
    <url-pattern>/Login</url-pattern>  
  </servlet-mapping>  
    
  <!-- Register Section -->
  <servlet>
    <servlet-name>Register</servlet-name>  
    <servlet-class>com.rp.system.login.Register</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>Register</servlet-name>  
    <url-pattern>/Register</url-pattern>  
  </servlet-mapping>  
  
    <!-- PW Reset Section -->
  <servlet>
    <servlet-name>Reset</servlet-name>  
    <servlet-class>com.rp.system.login.Reset</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>Reset</servlet-name>  
    <url-pattern>/Reset</url-pattern>  
  </servlet-mapping>  
  
    <!-- Logout Section -->
  <servlet>  
    <servlet-name>Logout</servlet-name>  
    <servlet-class>com.rp.system.login.Logout</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>Logout</servlet-name>  
    <url-pattern>/Logout</url-pattern>  
  </servlet-mapping>  
  
      <!-- Access Section -->
  <servlet>  
    <servlet-name>Access</servlet-name>  
